<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?><?import javafx.scene.control.Button?><?import javafx.scene.control.Label?><?import javafx.scene.control.TableColumn?><?import javafx.scene.control.TableView?><?import javafx.scene.control.TextArea?><?import javafx.scene.image.Image?><?import javafx.scene.image.ImageView?><?import javafx.scene.layout.HBox?><?import javafx.scene.layout.Region?><?import javafx.scene.layout.StackPane?><?import javafx.scene.layout.VBox?><?import javafx.scene.shape.Rectangle?><?import javafx.scene.text.Font?>
<VBox fx:id="errorContainerPane" prefHeight="480.0" prefWidth="640.0" stylesheets="@cardant.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.io7m.cardant.gui.internal.CAViewControllerError">
   <children>
      <HBox>
         <children>
            <StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="48.0" prefWidth="48.0">
               <children>
                  <Rectangle arcHeight="1.0" arcWidth="1.0" fill="#ffffff00" height="48.0" stroke="BLACK" strokeType="INSIDE" width="48.0" />
                  <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" smooth="false">
                     <image>
                        <Image url="@error32.png" />
                     </image>
                  </ImageView>
               </children>
            </StackPane>
            <Region maxWidth="-Infinity" minWidth="-Infinity" prefWidth="16.0" />
            <VBox HBox.hgrow="ALWAYS">
               <children>
                  <Label text="%error.header">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
                  <Region maxHeight="-Infinity" minHeight="-Infinity" prefHeight="16.0" />
                  <Label fx:id="errorMessage" />
               </children>
            </VBox>
         </children>
      </HBox>
      <TableView fx:id="errorTableView" prefHeight="64.0" styleClass="noHeader" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="errorNameColumn" editable="false" prefWidth="75.0" sortable="false" />
          <TableColumn fx:id="errorValueColumn" prefWidth="75.0" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <VBox.margin>
            <Insets top="16.0" />
         </VBox.margin>
      </TableView>
      <TextArea fx:id="errorDetails" editable="false" wrapText="true" VBox.vgrow="ALWAYS">
         <font>
            <Font name="Monospaced Regular" size="11.0" />
         </font>
         <VBox.margin>
            <Insets top="16.0" />
         </VBox.margin></TextArea>
      <Region layoutX="26.0" layoutY="106.0" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="16.0" VBox.vgrow="ALWAYS" />
      <HBox maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0">
         <children>
            <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="dismiss" defaultButton="true" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onDismissSelected" prefHeight="32.0" prefWidth="128.0" text="%error.dismiss" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
   </padding>
</VBox>
