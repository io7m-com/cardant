<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="itemEditorContainer" prefHeight="480.0" prefWidth="640.0" stylesheets="@cardant.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.io7m.cardant.gui.internal.CAViewControllerItemEditorLocationsTab">
<children>
  <TableView fx:id="locationsTableView" minHeight="128.0" prefHeight="400.0" prefWidth="200.0" styleClass="metadataTable" VBox.vgrow="ALWAYS">
    <columns>
      <TableColumn prefWidth="75.0" text="%location" />
      <TableColumn prefWidth="75.0" text="%count" />
    </columns>
    <columnResizePolicy>
      <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
    </columnResizePolicy>
  </TableView>
</children>
<padding>
  <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
</padding>
</VBox>
