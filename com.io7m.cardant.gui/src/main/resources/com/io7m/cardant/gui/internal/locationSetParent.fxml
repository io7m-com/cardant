<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="800.0" stylesheets="@cardant.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.io7m.cardant.gui.internal.CAViewControllerLocationSetParent">
   <children>
      <TreeView fx:id="locationTreeView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
      <Region maxHeight="-Infinity" minHeight="-Infinity" prefHeight="16.0" />
      <HBox maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0">
         <children>
            <Button fx:id="cancelButton" cancelButton="true" maxHeight="-Infinity" minHeight="-Infinity" mnemonicParsing="false" onAction="#onCancelSelected" prefHeight="32.0" prefWidth="128.0" text="%cancel" />
            <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="selectButton" defaultButton="true" disable="true" maxHeight="-Infinity" minHeight="-Infinity" mnemonicParsing="false" onAction="#onSelectSelected" prefHeight="32.0" prefWidth="128.0" text="%locationParent.select" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="16.0" left="16.0" right="16.0" top="16.0" />
   </padding>
</VBox>
