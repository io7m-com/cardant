      <Table type="parameterTable">
        <Columns>
          <Column>Parameter</Column>
          <Column>Type</Column>
          <Column>Cardinality</Column>
          <Column>Default</Column>
          <Column>Description</Column>
        </Columns>
        <Row>
          <Cell>
            <Term type="parameter">--count</Term>
          </Cell>
          <Cell>
            <Term type="type">java.lang.Long</Term>
          </Cell>
          <Cell>
            <Term type="expression">[1, 1]</Term>
          </Cell>
          <Cell>
            <Term type="constant"/>
          </Cell>
          <Cell>The number of instances of the item to add.</Cell>
        </Row>
        <Row>
          <Cell>
            <Term type="parameter">--item</Term>
          </Cell>
          <Cell>
            <Term type="type">com.io7m.cardant.model.CAItemID</Term>
          </Cell>
          <Cell>
            <Term type="expression">[1, 1]</Term>
          </Cell>
          <Cell>
            <Term type="constant"/>
          </Cell>
          <Cell>The item ID.</Cell>
        </Row>
        <Row>
          <Cell>
            <Term type="parameter">--location</Term>
          </Cell>
          <Cell>
            <Term type="type">com.io7m.cardant.model.CALocationID</Term>
          </Cell>
          <Cell>
            <Term type="expression">[1, 1]</Term>
          </Cell>
          <Cell>
            <Term type="constant"/>
          </Cell>
          <Cell>The location ID.</Cell>
        </Row>
      </Table>
    </FormalItem>
