<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Section id="8d5ce3ba-376b-3e4f-8e68-d5ecadc2b146" title="type-package-get-text" xmlns="urn:com.io7m.structural:8:0">
  <Subsection title="Name">
    <Paragraph>
      <Term type="command">type-package-get-text</Term>
       - Retrieve the text of a type package.
    </Paragraph>
  </Subsection>
  <Subsection title="Description">
    <Paragraph>
      The 
      <Term type="command">type-package-get-text</Term>
       command retrieves the raw text of an installed type package.
    </Paragraph>
    <FormalItem title="Parameters">
      <Table type="parameterTable">
        <Columns>
          <Column>Parameter</Column>
          <Column>Type</Column>
          <Column>Cardinality</Column>
          <Column>Default</Column>
          <Column>Description</Column>
        </Columns>
        <Row>
          <Cell>
            <Term type="parameter">--name</Term>
          </Cell>
          <Cell>
            <Term type="type">com.io7m.lanark.core.RDottedName</Term>
          </Cell>
          <Cell>
            <Term type="expression">[1, 1]</Term>
          </Cell>
          <Cell>
            <Term type="constant"/>
          </Cell>
          <Cell>The type package name.</Cell>
        </Row>
        <Row>
          <Cell>
            <Term type="parameter">--output</Term>
          </Cell>
          <Cell>
            <Term type="type">java.nio.file.Path</Term>
          </Cell>
          <Cell>
            <Term type="expression">[0, 1]</Term>
          </Cell>
          <Cell>
            <Term type="constant"/>
          </Cell>
          <Cell>The output file.</Cell>
        </Row>
        <Row>
          <Cell>
            <Term type="parameter">--version</Term>
          </Cell>
          <Cell>
            <Term type="type">com.io7m.verona.core.Version</Term>
          </Cell>
          <Cell>
            <Term type="expression">[1, 1]</Term>
          </Cell>
          <Cell>
            <Term type="constant"/>
          </Cell>
          <Cell>The type package version.</Cell>
        </Row>
      </Table>
    </FormalItem>
  </Subsection>
  <Subsection title="Examples">
    <FormalItem title="Example" type="example">
      <Verbatim><![CDATA[
[cardant]$ type-package-get-text --name com.io7m.example --version 1.0.0 --output file.xml
]]></Verbatim>
    </FormalItem>
  </Subsection>
</Section>

