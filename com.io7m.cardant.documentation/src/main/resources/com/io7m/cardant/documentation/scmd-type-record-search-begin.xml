<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Section id="5bd64820-cc42-304e-8fc6-6daa8a44ce42"
         title="type-record-search-begin"
         xmlns="urn:com.io7m.structural:8:0"
         xmlns:xi="http://www.w3.org/2001/XInclude">
  <Subsection title="Name">
    <Paragraph>
      <Term type="command">type-record-search-begin</Term>
       - Start searching for types
    </Paragraph>
  </Subsection>
  <Subsection title="Description">
    <Paragraph>
      The 
      <Term type="command">type-record-search-begin</Term>
       command starts <Link target="a14610db-3fbe-46b2-91a6-23d861f8d454">searching</Link> for types.
    </Paragraph>
    <Paragraph>
      Types can be filtered based on matching their names against a given
      <Link target="733cf6a3-dedc-4a3c-ae1c-b8301066ece3">name match</Link> expression.
    </Paragraph>
    <Paragraph>
      Types can be filtered based on matching their descriptions against a given
      <Link target="29c4727f-4858-480b-a137-b2328428cbe3">description match</Link> expression.
    </Paragraph>
  </Subsection>
  <xi:include href="scmd-type-record-search-begin-parameters.xml"/>
</Section>

